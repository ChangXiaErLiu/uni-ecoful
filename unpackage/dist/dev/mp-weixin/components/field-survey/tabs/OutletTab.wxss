
	/* 基础样式 */
.outlet-tab.data-v-d74124e6 {
		width: 100%;
		min-height: 100vh;
		background: linear-gradient(135deg, #f8fafc 0%, #f1f5f9 100%);
		padding: 10rpx 0 0 0;
		box-sizing: border-box;
}
.section-card.data-v-d74124e6 {
		background: #ffffff;
		border-radius: 20rpx;
		box-shadow: 0 4rpx 20rpx rgba(0, 0, 0, 0.05);
		overflow: hidden;
		border: 1rpx solid #e2e8f0;
}

	/* 头部样式 */
.section-header.data-v-d74124e6 {
		display: flex;
		justify-content: space-between;
		align-items: center;
		padding: 28rpx 32rpx;
		background: linear-gradient(to right, #f0fdf4, #dcfce7);
		border-bottom: 1rpx solid #bbf7d0;
}
.header-left.data-v-d74124e6 {
		display: flex;
		align-items: center;
		gap: 12rpx;
}
.section-title.data-v-d74124e6 {
		font-size: 32rpx;
		font-weight: 600;
		color: #166534;
		letter-spacing: 0.5rpx;
}
.record-count.data-v-d74124e6 {
		font-size: 26rpx;
		color: #4ade80;
		background: rgba(34, 197, 94, 0.1);
		padding: 8rpx 16rpx;
		border-radius: 20rpx;
}

	/* 主体内容 */
.section-body.data-v-d74124e6 {
		padding: 32rpx;
		display: flex;
		flex-direction: column;
		gap: 32rpx;
}

	/* 搜索和操作栏 */
.search-action-bar.data-v-d74124e6 {
		display: flex;
		flex-direction: column;
