/* 层级规范（全局使用） */
.compliance.data-v-febd4183 {
  display: flex;
  flex-direction: column;
  gap: 32rpx;
  min-height: 100vh;
  padding: 32rpx;
  background: linear-gradient(135deg, #f8fafc 0%, #f1f5f9 100%);
}
.compliance__header.data-v-febd4183 {
  display: flex;
  flex-direction: column;
  gap: 8rpx;
}
.compliance__title-section.data-v-febd4183 {
  display: flex;
  flex-direction: column;
  gap: 8rpx;
}
.compliance__title.data-v-febd4183 {
  font-size: 48rpx;
  font-weight: 700;
  color: #0f172a;
  line-height: 1.2;
}
.compliance__subtitle.data-v-febd4183 {
  font-size: 28rpx;
  color: #64748b;
  line-height: 1.4;
}
.compliance__content.data-v-febd4183 {
  display: flex;
  gap: 32rpx;
  flex: 1;
}

/* 上传卡片样式 */
.compliance__upload-card.data-v-febd4183 {
  flex: 1;
  background: #ffffff;
  border-radius: 24rpx;
  padding: 32rpx;
  box-shadow: 0 4rpx 24rpx rgba(15, 23, 42, 0.08);
  border: 1rpx solid #f1f5f9;
  max-width: 480rpx;
}
.compliance__result-card.data-v-febd4183 {
  flex: 2;
  background: #ffffff;
  border-radius: 24rpx;
  padding: 32rpx;
  box-shadow: 0 4rpx 24rpx rgba(15, 23, 42, 0.08);
  border: 1rpx solid #f1f5f9;
  display: flex;
  flex-direction: column;
  gap: 32rpx;
}
.compliance__section-header.data-v-febd4183 {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 8rpx;
}
.compliance__section-title.data-v-febd4183 {
  font-size: 32rpx;
  font-weight: 600;
  color: #0f172a;
}
.compliance__section-tip.data-v-febd4183,
.compliance__issues-count.data-v-febd4183 {
  font-size: 24rpx;
  color: #64748b;
}

/* 上传区域样式 */
.compliance__dropzone.data-v-febd4183 {
  border: 2rpx dashed #cbd5e1;
  border-radius: 24rpx;
  padding: 60rpx 32rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 20rpx;
  background: #fafafa;
  transition: all 0.3s ease;
  margin-top: 16rpx;
}
.compliance__dropzone.data-v-febd4183:active {
  background: #f1f5f9;
  border-color: #3b82f6;
}
.compliance__dropzone-icon.data-v-febd4183 {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background: rgba(59, 130, 246, 0.1);
  display: flex;
  align-items: center;
  justify-content: center;
}
.compliance__dropzone-title.data-v-febd4183 {
  font-size: 32rpx;
  font-weight: 600;
  color: #0f172a;
}
.compliance__dropzone-text.data-v-febd4183 {
  font-size: 26rpx;
  color: #64748b;
  text-align: center;
}

/* 文件预览样式 */
.compliance__file-preview.data-v-febd4183 {
  margin-top: 24rpx;
  padding: 24rpx;
  border-radius: 16rpx;
  background: #f8fafc;
  border: 1rpx solid #e2e8f0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  transition: all 0.2s ease;
}
.compliance__file-preview.data-v-febd4183:active {
  background: #f1f5f9;
}
.compliance__file-info.data-v-febd4183 {
  display: flex;
  align-items: center;
  gap: 16rpx;
  flex: 1;
}
.compliance__file-icon.data-v-febd4183 {
  width: 48rpx;
  height: 48rpx;
  border-radius: 12rpx;
  background: rgba(59, 130, 246, 0.1);
  display: flex;
  align-items: center;
  justify-content: center;
}
.compliance__file-details.data-v-febd4183 {
  display: flex;
  flex-direction: column;
  gap: 4rpx;
}
.compliance__file-name.data-v-febd4183 {
  font-size: 28rpx;
  font-weight: 600;
  color: #0f172a;
}
.compliance__file-meta.data-v-febd4183 {
  font-size: 24rpx;
  color: #64748b;
}
.compliance__file-actions.data-v-febd4183 {
  display: flex;
  align-items: center;
  gap: 16rpx;
}
.compliance__file-action.data-v-febd4183 {
  padding: 12rpx;
  border-radius: 8rpx;
  transition: all 0.2s ease;
}
.compliance__file-action.data-v-febd4183:active {
  background: #f1f5f9;
}

/* 状态指示器 */
.compliance__status-indicator.data-v-febd4183 {
  font-size: 24rpx;
  font-weight: 600;
  padding: 8rpx 16rpx;
  border-radius: 8rpx;
}
.compliance__status-indicator--idle.data-v-febd4183 {
  background: #f1f5f9;
  color: #64748b;
}
.compliance__status-indicator--processing.data-v-febd4183 {
  background: rgba(59, 130, 246, 0.1);
  color: #3b82f6;
}
.compliance__status-indicator--completed.data-v-febd4183 {
  background: rgba(16, 185, 129, 0.1);
  color: #059669;
}

/* 进度步骤样式 */
.compliance__progress.data-v-febd4183 {
  display: flex;
  flex-direction: column;
  gap: 0;
}
.compliance__progress-item.data-v-febd4183 {
  display: flex;
  align-items: flex-start;
  gap: 20rpx;
  padding: 32rpx 0;
  position: relative;
}
.compliance__progress-indicator.data-v-febd4183 {
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 0;
  flex-shrink: 0;
}
.compliance__progress-dot.data-v-febd4183 {
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  background: #e2e8f0;
  border: 4rpx solid #ffffff;
  box-shadow: 0 0 0 2rpx #e2e8f0;
  display: flex;
  align-items: center;
  justify-content: center;
  transition: all 0.3s ease;
}
.compliance__progress-line.data-v-febd4183 {
  width: 2rpx;
  flex: 1;
  background: #e2e8f0;
  margin: 8rpx 0;
  transition: all 0.3s ease;
}
.compliance__progress-item--active .compliance__progress-dot.data-v-febd4183 {
  background: #3b82f6;
  box-shadow: 0 0 0 2rpx #3b82f6;
  animation: pulse-febd4183 2s infinite;
}
.compliance__progress-item--completed .compliance__progress-dot.data-v-febd4183 {
  background: #10b981;
  box-shadow: 0 0 0 2rpx #10b981;
}
.compliance__progress-item--completed .compliance__progress-line.data-v-febd4183 {
  background: #10b981;
}
.compliance__progress-content.data-v-febd4183 {
  display: flex;
  flex-direction: column;
  gap: 8rpx;
  padding-top: 4rpx;
}
.compliance__progress-name.data-v-febd4183 {
  font-size: 28rpx;
  font-weight: 600;
  color: #0f172a;
}
.compliance__progress-status.data-v-febd4183 {
  font-size: 24rpx;
  color: #64748b;
}
.compliance__progress-item--active .compliance__progress-name.data-v-febd4183 {
  color: #3b82f6;
}
.compliance__progress-item--completed .compliance__progress-name.data-v-febd4183 {
  color: #10b981;
}
.compliance__progress-item--completed .compliance__progress-status.data-v-febd4183 {
  color: #10b981;
}

/* 操作按钮样式 */
.compliance__actions.data-v-febd4183 {
  display: flex;
  justify-content: flex-end;
  gap: 20rpx;
}
.compliance__button.data-v-febd4183 {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 12rpx;
  padding: 0 40rpx;
  height: 88rpx;
  border-radius: 20rpx;
  font-size: 28rpx;
  font-weight: 600;
  border: none;
  transition: all 0.3s ease;
}
.compliance__button.data-v-febd4183:active {
  transform: translateY(2rpx);
}
.compliance__button--primary.data-v-febd4183 {
  background: linear-gradient(135deg, #3b82f6, #2563eb);
  color: #ffffff;
  box-shadow: 0 4rpx 20rpx rgba(59, 130, 246, 0.3);
}
.compliance__button--primary.data-v-febd4183:active {
  box-shadow: 0 2rpx 12rpx rgba(59, 130, 246, 0.4);
}
.compliance__button--primary.data-v-febd4183:disabled {
  background: #cbd5e1;
  box-shadow: none;
  transform: none;
}
.compliance__button--secondary.data-v-febd4183 {
  background: #ffffff;
  color: #64748b;
  border: 2rpx solid #e2e8f0;
}
.compliance__button--secondary.data-v-febd4183:active {
  background: #f8fafc;
}

/* 问题列表样式 */
.compliance__issues.data-v-febd4183 {
  display: flex;
  flex-direction: column;
  gap: 24rpx;
}
.compliance__issue-list.data-v-febd4183 {
  border-radius: 16rpx;
  background: #f8fafc;
  padding: 0;
}
.compliance__issue-item.data-v-febd4183 {
  padding: 24rpx;
  border-bottom: 1rpx solid #e2e8f0;
  transition: all 0.2s ease;
}
.compliance__issue-item.data-v-febd4183:active {
  background: #f1f5f9;
}
.compliance__issue-item.data-v-febd4183:last-child {
  border-bottom: none;
}
.compliance__issue-item--high.data-v-febd4183 {
  border-left: 4rpx solid #ef4444;
}
.compliance__issue-item--medium.data-v-febd4183 {
  border-left: 4rpx solid #f59e0b;
}
.compliance__issue-item--low.data-v-febd4183 {
  border-left: 4rpx solid #3b82f6;
}
.compliance__issue-header.data-v-febd4183 {
  display: flex;
  align-items: center;
  gap: 12rpx;
  margin-bottom: 12rpx;
}
.compliance__issue-type-badge.data-v-febd4183 {
  font-size: 22rpx;
  font-weight: 600;
  padding: 6rpx 12rpx;
  border-radius: 6rpx;
}
.compliance__issue-type-badge--logic.data-v-febd4183 {
  background: rgba(239, 68, 68, 0.1);
  color: #dc2626;
}
.compliance__issue-type-badge--policy.data-v-febd4183 {
  background: rgba(245, 158, 11, 0.1);
  color: #d97706;
}
.compliance__issue-type-badge--data.data-v-febd4183 {
  background: rgba(59, 130, 246, 0.1);
  color: #2563eb;
}
.compliance__issue-type-badge--format.data-v-febd4183 {
  background: rgba(107, 114, 128, 0.1);
  color: #6b7280;
}
.compliance__issue-type-badge--semantic.data-v-febd4183 {
  background: rgba(139, 92, 246, 0.1);
  color: #7c3aed;
}
.compliance__issue-location.data-v-febd4183 {
  font-size: 24rpx;
  color: #64748b;
}
.compliance__issue-desc.data-v-febd4183 {
  font-size: 28rpx;
  color: #0f172a;
  line-height: 1.5;
  margin-bottom: 12rpx;
  display: block;
}
.compliance__issue-suggestion.data-v-febd4183 {
  display: flex;
  gap: 8rpx;
  padding: 16rpx;
  background: rgba(16, 185, 129, 0.05);
  border-radius: 8rpx;
  border-left: 4rpx solid #10b981;
}
.compliance__issue-suggestion-label.data-v-febd4183 {
  font-size: 24rpx;
  font-weight: 600;
  color: #059669;
  flex-shrink: 0;
}
.compliance__issue-suggestion-text.data-v-febd4183 {
  font-size: 24rpx;
  color: #059669;
  line-height: 1.4;
}

/* 空状态样式 */
.compliance__empty-state.data-v-febd4183 {
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 16rpx;
  padding: 80rpx 32rpx;
  color: #cbd5e1;
}
.compliance__empty-text.data-v-febd4183 {
  font-size: 28rpx;
  color: #64748b;
  font-weight: 600;
}
.compliance__empty-subtext.data-v-febd4183 {
  font-size: 24rpx;
  color: #94a3b8;
}

/* 报告下载区域 */
.compliance__report-section.data-v-febd4183 {
  padding-top: 24rpx;
  border-top: 1rpx solid #f1f5f9;
}
.compliance__download-button.data-v-febd4183 {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 12rpx;
  width: 100%;
  height: 88rpx;
  background: rgba(59, 130, 246, 0.1);
  border: 2rpx dashed #3b82f6;
  border-radius: 16rpx;
  color: #3b82f6;
  font-size: 28rpx;
  font-weight: 600;
  transition: all 0.3s ease;
}
.compliance__download-button.data-v-febd4183:active {
  background: rgba(59, 130, 246, 0.2);
}

/* 动画 */
@keyframes pulse-febd4183 {
0% {
    box-shadow: 0 0 0 0 rgba(59, 130, 246, 0.4);
}
70% {
    box-shadow: 0 0 0 10rpx rgba(59, 130, 246, 0);
}
100% {
    box-shadow: 0 0 0 0 rgba(59, 130, 246, 0);
}
}
/* 响应式设计 */
@media (max-width: 1023px) {
.compliance.data-v-febd4183 {
    padding: 24rpx;
    gap: 24rpx;
}
.compliance__content.data-v-febd4183 {
    flex-direction: column;
}
.compliance__upload-card.data-v-febd4183 {
    max-width: none;
}
.compliance__result-card.data-v-febd4183 {
    gap: 24rpx;
}
}
/* 桌面端悬停效果 */
@media (min-width: 768px) {
.compliance__dropzone.data-v-febd4183:hover {
    border-color: #3b82f6;
    background: #f8fafc;
}
.compliance__file-preview.data-v-febd4183:hover {
    background: #f1f5f9;
}
.compliance__button--primary.data-v-febd4183:hover {
    box-shadow: 0 6rpx 24rpx rgba(59, 130, 246, 0.4);
}
.compliance__button--secondary.data-v-febd4183:hover {
    background: #f8fafc;
    border-color: #cbd5e1;
}
.compliance__issue-item.data-v-febd4183:hover {
    background: #f1f5f9;
}
.compliance__download-button.data-v-febd4183:hover {
    background: rgba(59, 130, 246, 0.2);
}
}